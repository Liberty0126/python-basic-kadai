class Human:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def say_nameandage(self):
        print(f"私は{self.name}です。{self.age}歳です。")

Robert = Human(name='ロバート', age='34')
Thomas = Human(name='トーマス', age='29')

Robert.say_nameandage()
Thomas.say_nameandage()
